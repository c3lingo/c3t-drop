{
  "name": "c3t-drop",
  "version": "0.1.0",
  "description": "A drop point for material that helps us translate c3 talks",
  "main": "yarn start",
  "scripts": {
    "start": "NODE_ENV=production ts-node index.ts",
    "test": "jest",
    "fix": "prettier --write index.ts \"lib/**/*.ts\" \"models/**/*.ts\" && tslint --project tsconfig.json --fix \"lib/**/*.ts\" \"models/**/*.ts\""
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "author": "Philipp Bock <p@philippbock.de> (http://philippbock.de/)",
  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/basic-auth": "^1.1.6",
    "@types/bunyan": "^1.8.11",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.10",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "basic-auth": "^2.0.1",
    "bootstrap": "5.3.2",
    "bunyan": "^1.8.15",
    "chokidar": "^3.5.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fs-promise": "^2.0.3",
    "helmet": "^7.1.0",
    "i18n": "^0.15.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "pug": "^3.0.2",
    "pushover-notifications": "^1.2.2"
  }
}
